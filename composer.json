{
    "name": "karpov/backuper",
    "description": "Backup Tools",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Сергей Карпов",
            "email": "karpoff.s@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "symfony/console": "^4.1",
        "symfony/lock": "^4.1",
        "symfony/yaml": "^4.1",
        "monolog/monolog": "^1.23",
        "symfony/process": "^4.1",
        "symfony/finder": "^4.1",
        "symfony/dotenv": "^4.1",
        "league/flysystem": "^1.0",
        "league/flysystem-sftp": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.1"
    },
    "suggest": {
        "ext-fileinfo": "Required for MimeType",
        "ext-ftp": "Required for FTP and SFTP"
    }
}
